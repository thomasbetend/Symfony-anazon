{% extends 'base.html.twig' %}

{% block title %}catégories{% endblock %}

{% block body %}
<div class="main w-75">
    <h1>Catégories</h1>
        {% for category in categories %}
            <ul>
                <li><a href="{{ path('app_category', {id: category.id}) }}">{{ category.title }}</a></li>
            </ul>
        {% endfor %}
</div>
{% endblock %}
