<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        <script src="https://kit.fontawesome.com/5474cfcdca.js" crossorigin="anonymous"></script>

        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% include '_parts/above-part.html.twig'%}
        {% include '_parts/nav.html.twig' %}
        {% include '_parts/grey-menu.html.twig'%}

        

        {% block body %}
            {% block arborescence %}
                <div class="menu-chevron">
                    <a href="{{ path('app_categories') }}">
                        <div class="menu-chevron-item">
                            <p>Accueil</p>
                            <i class="fa-solid fa-chevron-right chevron1"></i>
                        </div>
                    </a>
            {% endblock %}
        {% endblock %}
    </body>
</html>
